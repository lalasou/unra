<?xml version="1.0"?>
<Container version="2">
  <Name>RSSHub</Name>
  <Repository>diygod/rsshub:2022-04-04</Repository>
  <Registry>https://hub.docker.com/r/diygod/rsshub</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/DIYgod/RSSHub/discussions</Support>
  <Project>https://github.com/DIYgod/RSSHub</Project>
  <Overview>RSSHub is an open source, easy to use, and extensible RSS feed generator. It's capable of generating RSS feeds from pretty much everything.</Overview>
  <Category>Tools:Utilities</Category>
  <WebUI>http://[IP]:[PORT:1200]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/MyFaith/unraid-templates/master/rsshub/rsshub.xml</TemplateURL>
  <Icon>https://i.loli.net/2019/04/23/5cbeb7e41414c.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1649089982</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>RSSHub is an open source, easy to use, and extensible RSS feed generator. It's capable of generating RSS feeds from pretty much everything.</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>1200</HostPort>
        <ContainerPort>1200</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data/>
  <Environment>
    <Variable>
      <Value>memory</Value>
      <Name>CACHE_TYPE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>60</Value>
      <Name>CACHE_EXPIRE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>redis://localhost:6379/</Value>
      <Name>REDIS_URL</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Web UI Port" Target="1200" Default="1200" Mode="tcp" Description="Web UI Port" Type="Port" Display="always" Required="false" Mask="false">1200</Config>
  <Config Name="Cache Type" Target="CACHE_TYPE" Default="memory" Mode="" Description="Cache type, memory or redis, empty this value will disable caching, default to memory" Type="Variable" Display="advanced" Required="false" Mask="false">memory</Config>
  <Config Name="Cache Expire" Target="CACHE_EXPIRE" Default="5 * 60" Mode="" Description="Route cache expiry time in seconds, default to 5 * 60" Type="Variable" Display="advanced" Required="false" Mask="false">60</Config>
  <Config Name="Redis URL" Target="REDIS_URL" Default="redis://localhost:6379/" Mode="" Description="Redis target address (invalid when CACHE_TYPE is set to memory), default to redis://localhost:6379/" Type="Variable" Display="advanced" Required="false" Mask="false">redis://localhost:6379/</Config>
</Container>
